{
  "name": "@my-org/svg-toolkit",
  "packageManager": "yarn@3.2.0",
  "version": "0.0.1",
  "description": "Build optimized SVG sprites without pain",
  "source": "src/index.ts",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "default": "./dist/index.mjs",
      "import": "./dist/index.mjs"
    }
  },
  "bin": {
    "sprite": "bin.mjs"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "library": {
    "minify": false
  },
  "sideEffects": false,
  "scripts": {
    "version": "yarn version",
    "clean": "yarn dlx rimraf dist",
    "build": "library build",
    "lint": "eslint src",
    "dev": "library watch"
  },
  "devDependencies": {
    "@my-org/config": "workspace:*",
    "@my-org/library-toolkit": "workspace:*",
    "@swc/helpers": "0.4.14",
    "@types/eslint": "^8.4.10",
    "@types/prettier": "^2.7.1",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@types/svgo": "^3.0.0",
    "eslint": "8.30.0",
    "eslint-kit": "6.10.0",
    "prettier": "2.8.1",
    "typescript": "4.9.4"
  },
  "peerDependencies": {
    "prettier": "^2"
  },
  "peerDependenciesMeta": {
    "prettier": {
      "optional": true
    }
  },
  "nx": {
    "targets": {
      "dev": {
        "dependsOn": [
          "^build",
          "dev"
        ]
      }
    }
  },
  "dependencies": {
    "cosmiconfig": "8.0.0",
    "ora": "6.1.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sade": "1.8.1",
    "svgo": "3.0.2",
    "svgson": "5.2.1",
    "tiny-glob": "0.2.9"
  }
}
