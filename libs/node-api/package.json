{
  "name": "@my-org/node-api",
  "description": "Collection of NodeJS APIs wrappers",
  "packageManager": "yarn@3.2.0",
  "version": "0.0.1",
  "source": "src/index.ts",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    "require": "./dist/index.cjs",
    "default": "./dist/index.mjs",
    "import": "./dist/index.mjs"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "sideEffects": false,
  "scripts": {
    "version": "yarn version",
    "clean": "yarn dlx rimraf dist",
    "build": "library build",
    "lint": "eslint src",
    "dev": "library watch"
  },
  "devDependencies": {
    "@my-org/config": "workspace:*",
    "@my-org/library-toolkit": "workspace:*",
    "@swc/helpers": "0.4.3",
    "@types/node": "18.7.2",
    "eslint": "8.21.0",
    "eslint-kit": "6.1.1",
    "typescript": "4.7.4"
  }
}
