{
  "name": "@my-org/template-library",
  "packageManager": "yarn@3.2.0",
  "version": "0.0.1",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "default": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "sideEffects": false,
  "scripts": {
    "version": "yarn version",
    "clean": "yarn dlx rimraf dist",
    "build": "rollup -c",
    "lint": "eslint src",
    "dev": "rollup -c --watch"
  },
  "devDependencies": {
    "@my-org/config": "workspace:*",
    "@my-org/rollup-config": "workspace:*",
    "@swc/helpers": "0.4.3",
    "@types/eslint": "^8.4.5",
    "eslint": "8.20.0",
    "eslint-kit": "5.10.0",
    "rollup": "2.77.0",
    "typescript": "4.7.4"
  }
}
